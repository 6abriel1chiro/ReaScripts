desc:WaitInput_tracker
version: 1.0
changelog: init
author: MPL
website: http://forum.cockos.com/member.php?u=70694
about:
  Tracking MIDI data
  
  
options:gmem=mpl_WaitInput


@init

@block
  while ( midirecv(offset,msg1,msg2,msg3)  ) (
      midisend(offset,msg1,msg2,msg3);
      gmem[1] = msg1;
    );
  

@sample
spl0=spl0;
spl1=spl1;
